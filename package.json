    "lint-staged": "13.2.0",
    "miragejs": "0.1.47",
    "postcss": "8.4.21",
    "sass": "1.59.3",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.29.7",
    "vue-tsc": "1.2.0"
